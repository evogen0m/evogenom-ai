{
  "name": "evogenomai",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main --migrate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "migrate:test": "NODE_ENV=test drizzle-kit migrate",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest run --inspect-brk --run-in-band",
    "test:e2e": "vitest run --config ./vitest.e2e.config.ts",
    "codegen": "graphql-codegen --config codegen.ts && pnpm lint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org evogenom --project node-nestjs ./dist && sentry-cli sourcemaps upload --org evogenom --project node-nestjs --release ${SENTRY_RELEASE:-$(git rev-parse HEAD)} ./dist",
    "sentry:release": "sentry-cli releases new ${SENTRY_RELEASE:-$(git rev-parse HEAD)} --org evogenom --project node-nestjs",
    "sentry:deploy": "sentry-cli releases deploys ${SENTRY_RELEASE:-$(git rev-parse HEAD)} new --env ${NODE_ENV:-production} --org evogenom --project node-nestjs",
    "sentry:finalize": "sentry-cli releases finalize ${SENTRY_RELEASE:-$(git rev-parse HEAD)} --org evogenom --project node-nestjs",
    "sentry:full": "pnpm sentry:release && pnpm build && pnpm sentry:sourcemaps && pnpm sentry:finalize && pnpm sentry:deploy",
    "docker:build": "docker build --build-arg SENTRY_RELEASE=${SENTRY_RELEASE:-$(git rev-parse HEAD)} -t evogenomai ."
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.799.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nestjs-cls/transactional": "^2.6.1",
    "@nestjs-cls/transactional-adapter-drizzle-orm": "^1.1.15",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.1.0",
    "@sentry/cli": "^2.43.0",
    "@sentry/nestjs": "^9.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.3.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "jose": "^5.10.0",
    "langfuse": "^3.37.2",
    "nestjs-cls": "^5.4.2",
    "nestjs-zod": "^4.3.1",
    "openai": "^4.91.1",
